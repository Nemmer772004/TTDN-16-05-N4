        <!-- Sequence for Equipment Borrowing -->
        <record id="seq_equipment_borrowing" model="ir.sequence">
            <field name="name">Equipment Borrowing</field>
            <field name="code">equipment.borrowing</field>
            <field name="prefix">EB</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Cham Cong -->
        <record id="seq_cham_cong" model="ir.sequence">
