            <field name="code">cham_cong</field>
            <field name="prefix">CC</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Don Nghi Phep -->
        <record id="seq_don_nghi_phep" model="ir.sequence">
            <field name="name">Đơn nghỉ phép</field>
            <field name="code">don_nghi_phep</field>
            <field name="prefix">DNP</field>
